@{
    ViewBag.Title = ViewBag.Message + " results:";
}
<h2>@ViewBag.Title</h2>
@*Loop through results list and print*@
<ul>
    @if (ViewBag.Results != null && ViewBag.Results != "")
    {
        foreach (var item in ViewBag.Results)
        {
            <li>@item</li>
        }
    }
    else
    {
        <li>@ViewBag.Error</li>
    }

</ul>

